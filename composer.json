{
    "name": "nmaier95/craft-fields-vcs",
    "description": "Export fields as json to get them into your vcs of choice.  Import them into your cms instance as well.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft-fields-vcs"
    ],
    "support": {
        "docs": "https://github.com/nmaier95/craft-fields-vcs/blob/master/README.md",
        "issues": "https://github.com/nmaier95/craft-fields-vcs/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Niklas Maier",
            "homepage": "https://github.com/nmaier95"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "nmaier95\\craftfieldsvcs\\": "src/"
        }
    },
    "extra": {
        "name": "craft-fields-vcs",
        "handle": "craft-fields-vcs",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/nmaier95/craft-fields-vcs/master/CHANGELOG.md",
        "class": "nmaier95\\craftfieldsvcs\\Craftfieldsvcs"
    }
}
